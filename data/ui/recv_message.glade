<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="SuiRecvMessage" parent="SuiMessage">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="halign">start</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkBox" id="frame_box">
        <property name="can_focus">0</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox" id="sender_box">
            <property name="can_focus">0</property>
            <property name="spacing">2</property>
            <child>
              <object class="GtkEventBox" id="sender_event_box">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <child>
                  <object class="GtkLabel" id="sender_label">
                    <property name="can_focus">0</property>
                    <property name="label">Unable to display nickname</property>
                    <property name="wrap">1</property>
                    <property name="lines">0</property>
                    <attributes>
                      <attribute name="weight" value="semibold"></attribute>
                    </attributes>
                    <style>
                      <class name="sui-message-nick-label"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="remark_label">
                <property name="halign">center</property>
                <property name="can_focus">0</property>
                <property name="use_underline">1</property>
                <attributes>
                  <attribute name="weight" value="normal"></attribute>
                </attributes>
                <style>
                  <class name="sui-message-subnick-label"/>
                  <class name="dim-label"/>
                </style>
              </object>
            </child>
            <style>
              <class name="sui-user-box"/>
            </style>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="can_focus">0</property>
            <child>
              <object class="GtkBox" id="content_box">
                <property name="can_focus">0</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkLabel" id="message_label">
                    <property name="can_focus">0</property>
                    <property name="halign">start</property>
                    <property name="label">&lt;span color=&quot;red&quot;&gt;Unable to display message content &lt;sup&gt;ERROR&lt;/sup&gt;&lt;/span&gt;</property>
                    <property name="use_markup">1</property>
                    <property name="wrap">1</property>
                    <property name="wrap_mode">word-char</property>
                    <property name="selectable">1</property>
                    <property name="max_width_chars">80</property>
                    <attributes>
                      <attribute name="stretch" value="normal"></attribute>
                    </attributes>
                    <style>
                      <class name="sui-message-label"/>
                    </style>
                  </object>
                </child>
                <style>
                  <class name="sui-message-content"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="time_label">
                <property name="name">time_label</property>
                <property name="can_focus">0</property>
                <property name="halign">end</property>
                <property name="valign">end</property>
                <property name="label">19:51</property>
                <style>
                  <class name="sui-message-time-label"/>
                </style>
              </object>
            </child>
          </object>
        </child>
        <style>
          <class name="sui-message-frame"/>
        </style>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="fill">True</property>
        <property name="position">2</property>
      </packing>
    </child>
    <style>
      <class name="sui-recv-message"/>
      <class name="sui-message"/>
    </style>
  </template>
</interface>
